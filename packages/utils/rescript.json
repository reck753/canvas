{
  "name": "@reck753/canvas-utils",
  "namespace": false,
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".mjs",
  "ignored-dirs": ["__generated__"],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "rescript-shims",
      "subdirs": true
    }
  ],
  "bs-dev-dependencies": [],
  "bs-dependencies": ["@rescript/core"],
  "gentypeconfig": {
    "language": "typescript",
    "shims": {
      "Js": "Js",
      "PervasivesU": "PervasivesU"
    },
    "generatedFileExtension": ".gen.ts",
    "importPath": "relative",
    "module": "commonjs",
    "debug": {
      "all": false,
      "basic": false
    }
  },
  "bsc-flags": ["-open RescriptCore"]
}
